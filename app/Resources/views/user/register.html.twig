{% extends 'base.html.twig' %}

{% block body %}




    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1>Register!</h1>

            {{ form_start(form) }}
                {{ form_row(form.email) }}
                {{ form_row(form.plainPassword.first,{'label':'pass'}) }}
                {{ form_row(form.plainPassword.second,{'label':'confirm pass'}) }}

                <button type="submit" class="btn btn-success" formnovalidate>register</button>
                {{ form_end(form) }}

            </div>
        </div>
    </div>







{% endblock %}